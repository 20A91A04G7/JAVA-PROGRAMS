# JAVA-PROGRAMS
package cond;
import java.util.*;
public class If_else2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("enter 2 numbers :");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = (a>b)?a:b;
		System.out.println("bigger number = "+c);

	}

}
